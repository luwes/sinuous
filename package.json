{
  "private": false,
  "name": "sinuous",
  "version": "0.30.0",
  "description": "ðŸ§¬ Small, fast, reactive render engine",
  "module": "src/index.js",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.js"
    },
    "./h": {
      "import": "./src/h.js"
    },
    "./babel-plugin-htm": {
      "import": "./src/babel-plugin-htm.js",
      "require": "./dist/babel-plugin-htm.js"
    },
    "./htm": {
      "import": "./src/htm.js"
    },
    "./hydrate": {
      "import": "./src/hydrate.js"
    },
    "./map": {
      "import": "./src/map.js"
    },
    "./observable": {
      "import": "./src/observable.js"
    },
    "./template": {
      "import": "./src/template.js"
    }
  },
  "files": [
    "src",
    "!**/test/**"
  ],
  "workspaces": {
    "packages": [
      "./",
      "site",
      "site/src/**/*"
    ]
  },
  "scripts": {
    "lint": "eslint 'src/*.js'",
    "test": "cross-env COVERAGE=true yarn test:htm && karma start",
    "test:htm": "tape -r esm test/htm/*.js | faucet",
    "dev": "(cd site && yarn dev)",
    "build": "rollup -i src/babel-plugin-htm.js -f cjs -d dist && (cd site && yarn build)",
    "prepublishOnly": "yarn lint"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/warnings"
    ],
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "rules": {
      "semi": "error",
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^hs?|ds?$"
        }
      ]
    }
  },
  "repository": "luwes/sinuous",
  "keywords": [
    "functional",
    "reactive",
    "declarative"
  ],
  "author": "Wesley Luyten <me@wesleyluyten.com> (https://wesleyluyten.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luwes/sinuous/issues"
  },
  "homepage": "https://github.com/luwes/sinuous#readme",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "ansi-colors": "^4.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-plugin-import": "^2.26.0",
    "esm": "^3.2.25",
    "faucet": "^0.0.1",
    "ispy": "^0.1.2",
    "karma": "6.4.0",
    "karma-coverage": "2.1.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-rollup-preprocessor": "7.0.7",
    "karma-tap": "^4.2.0",
    "karma-tap-pretty-reporter": "^4.2.0",
    "minimist": "^1.2.6",
    "rollup": "^2.79.0",
    "rollup-plugin-istanbul": "^3.0.0",
    "tape": "5.6.0",
    "tape-browser": "^4.13.0",
    "typescript": "^4.8.2"
  }
}
