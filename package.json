{
  "private": true,
  "name": "sinuous",
  "version": "0.28.3",
  "description": "ðŸ§¬ Small, fast, reactive render engine",
  "module": "module/sinuous.js",
  "main": "dist/sinuous.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "import": "./module/sinuous.js",
      "require": "./dist/sinuous.js"
    },
    "./all": {
      "import": "./module/all.js",
      "require": "./dist/all.js"
    },
    "./h": {
      "import": "./module/h.js",
      "require": "./dist/h.js"
    },
    "./babel-plugin-htm": {
      "import": "./module/babel-plugin-htm.js",
      "require": "./dist/babel-plugin-htm.js"
    },
    "./htm": {
      "import": "./module/htm.js",
      "require": "./dist/htm.js"
    },
    "./hydrate": {
      "import": "./module/hydrate.js",
      "require": "./dist/hydrate.js"
    },
    "./map": {
      "import": "./module/map.js",
      "require": "./dist/map.js"
    },
    "./observable": {
      "import": "./module/observable.js",
      "require": "./dist/observable.js"
    },
    "./template": {
      "import": "./module/template.js",
      "require": "./dist/template.js"
    }
  },
  "files": [
    "module",
    "dist",
    "src",
    "src/jsx.d.ts",
    "src/shared.d.ts"
  ],
  "workspaces": {
    "packages": [
      "./",
      "site",
      "site/src/**/*"
    ]
  },
  "scripts": {
    "lint": "eslint 'src/{*,*/*,}/src/**/*.js'",
    "format": "prettier --write '**/{src,test}/**/*.js' 'scripts/**/*.js' '**/*.md' '!**/build/**' '!**/tape/**' '!**/htm/**' '!**/babel-plugin-htm/**' '!**/site/**'",
    "test": "cross-env COVERAGE=true yarn test:htm && karma start",
    "test:htm": "tape -r esm src/htm/test/*.js | faucet",
    "test:performance": "PERSIST=true time node --expose-gc src/observable/test/perf/index.js",
    "coverage": "codecov",
    "build": "rollup -c scripts/rollup.js --silent",
    "watch": "rollup -wc scripts/rollup.js --silent",
    "serve": "(cd site && yarn dev)",
    "dev": "run-p watch serve",
    "http": "http-server -c-1 -p 8000 . ",
    "docs": "node scripts/docs.js && yarn format:docs",
    "format:docs": "prettier --write '**/*.md'",
    "prepublishOnly": "yarn lint && yarn build"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "semi": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/warnings"
    ],
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "rules": {
      "semi": "error",
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^hs?|ds?$"
        }
      ]
    }
  },
  "repository": "luwes/sinuous",
  "keywords": [
    "functional",
    "reactive",
    "declarative"
  ],
  "author": "Wesley Luyten <me@wesleyluyten.com> (https://wesleyluyten.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luwes/sinuous/issues"
  },
  "homepage": "https://github.com/luwes/sinuous#readme",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-transform-object-assign": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "ansi-colors": "^4.1.1",
    "babel-plugin-transform-jsx-to-htm": "^2.2.0",
    "browserify": "^17.0.0",
    "codecov": "^3.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.7.0",
    "esm": "^3.2.25",
    "faucet": "^0.0.1",
    "fs-extra": "^10.0.0",
    "http-server": "^14.1.0",
    "ispy": "^0.1.2",
    "jsdoc-to-markdown": "^7.1.1",
    "karma": "^6.3.16",
    "karma-coverage": "^2.1.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-rollup-preprocessor": "^7.0.7",
    "karma-sauce-launcher": "^4.1.5",
    "karma-tap": "^4.2.0",
    "karma-tap-pretty-reporter": "^4.2.0",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "observable": "^2.1.4",
    "prettier": "^2.5.1",
    "ramda": "^0.28.0",
    "rollup": "^2.66.1",
    "rollup-plugin-gzip": "^3.0.0",
    "rollup-plugin-istanbul": "^3.0.0",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "tap-spec": "^5.0.0",
    "tape": "^5.5.0",
    "tape-browser": "^4.13.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {}
}
